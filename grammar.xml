<!--5 -->
<Phrase>
  <TestPhrase>Карта Турции существует для СГ</TestPhrase>
  <Pattern>
    <Element><WordForm></WordForm><Category>С</Category><Grammems>им</Grammems></Element>
    <Element><WordForm></WordForm><Category>С</Category><Grammems>рд</Grammems></Element>
    <Element><WordForm></WordForm><Category>Г</Category><Grammems></Grammems></Element>
    <Element><WordForm>для</WordForm><Category>ПРЕДЛ</Category><Grammems></Grammems></Element>
    <Element><WordForm></WordForm><Category>С</Category><Grammems>рд</Grammems></Element>
  </Pattern>
  <TrasformationFormula>#2l [рд] #1l [кто] #5l [для-рд] #3[.]</TrasformationFormula>
</Phrase>


